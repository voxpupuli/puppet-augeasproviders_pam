{
  "name": "puppet-augeasproviders_pam",
  "version": "4.0.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Augeas-based PAM type and provider for Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-augeasproviders_pam",
  "project_page": "https://github.com/voxpupuli/puppet-augeasproviders_pam",
  "issues_url": "https://github.com/voxpupuli/puppet-augeasproviders_pam/issues",
  "description": "This module provides type/providers for PAM using the Augeas configuration API library.",
  "dependencies": [
    {
      "name": "puppet/augeasproviders_core",
      "version_requirement": ">= 2.4.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04",
        "22.04",
        "24.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">= 8.19.0 < 9.0.0"
    }
  ]
}
